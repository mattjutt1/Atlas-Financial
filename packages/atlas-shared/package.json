{
  "name": "@atlas/shared",
  "version": "1.0.0",
  "description": "Shared library for Atlas Financial modular monolith",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth/index.js",
      "require": "./dist/auth/index.js",
      "types": "./dist/auth/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.js",
      "require": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors/index.js",
      "require": "./dist/errors/index.js",
      "types": "./dist/errors/index.d.ts"
    },
    "./graphql": {
      "import": "./dist/graphql/index.js",
      "require": "./dist/graphql/index.js",
      "types": "./dist/graphql/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./security": {
      "import": "./dist/security/index.js",
      "require": "./dist/security/index.js",
      "types": "./dist/security/index.d.ts"
    },
    "./monitoring": {
      "import": "./dist/monitoring/index.js",
      "require": "./dist/monitoring/index.js",
      "types": "./dist/monitoring/index.d.ts"
    },
    "./database": {
      "import": "./dist/database/index.js",
      "require": "./dist/database/index.js",
      "types": "./dist/database/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@apollo/client": "^3.8.0",
    "graphql": "^16.8.0",
    "jsonwebtoken": "^9.0.0",
    "zod": "^3.22.0",
    "crypto-js": "^4.1.0",
    "lodash": "^4.17.21",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.1.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "next": ">=13.0.0",
    "supertokens-auth-react": ">=0.34.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "supertokens-auth-react": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlas-financial/atlas-shared.git"
  },
  "keywords": [
    "atlas",
    "financial",
    "shared",
    "library",
    "typescript",
    "banking",
    "fintech"
  ],
  "author": "Atlas Financial Team",
  "license": "PROPRIETARY"
}