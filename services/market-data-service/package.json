{
  "name": "@atlas-financial/market-data-service",
  "version": "1.0.0",
  "description": "Real-time market data service with WebSocket integration and AI-powered portfolio analysis",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t atlas-market-data-service .",
    "docker:run": "docker run -p 4000:4000 -p 8080:8080 atlas-market-data-service"
  },
  "keywords": [
    "market-data",
    "websocket",
    "real-time",
    "graphql",
    "portfolio-analysis",
    "ai",
    "financial-data"
  ],
  "author": "Atlas Financial",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^3.12.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "graphql": "^16.8.1",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^9.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "subscriptions-transport-ws": "^0.11.0",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.8.6",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atlas-financial/market-data-service.git"
  },
  "bugs": {
    "url": "https://github.com/atlas-financial/market-data-service/issues"
  }
}